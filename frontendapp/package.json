{
  "name": "learn-react-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:storybook": "react-native run-android --variant=storybookdebug",
    "android:storybook-release": "react-native run-android --variant=storybookrelease",
    "android:dev": "react-native run-android --variant=developmentdebug",
    "android:dev-release": "react-native run-android --variant=developmentrelease",
    "android:prod": "react-native run-android --variant=productiondebug",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "devtools": "react-devtools",
    "postinstall": "rndebugger-open",
    "storybook": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook -p 7007",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/datetimepicker": "^6.0.2",
    "@react-native-firebase/app": "^14.7.0",
    "@react-native-firebase/auth": "^14.7.0",
    "@react-native-firebase/firestore": "^14.7.0",
    "@react-native-firebase/storage": "^14.7.0",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/drawer": "^6.3.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "date-fns": "^2.28.0",
    "immer": "^9.0.12",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-native": "0.67.1",
    "react-native-calendars": "^1.1279.0",
    "react-native-config": "^1.4.5",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-get-random-values": "^1.7.2",
    "react-native-modal-datetime-picker": "^13.1.0",
    "react-native-reanimated": "^2.4.1",
    "react-native-safe-area-context": "^3.4.1",
    "react-native-screens": "^3.12.0",
    "react-native-vector-icons": "^9.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/runtime": "^7.16.7",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.7",
    "metro-react-native-babel-preset": "^0.67.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.23.0",
    "react-dom": "17.0.2",
    "react-native-debugger-open": "^0.3.25",
    "react-test-renderer": "17.0.2",
    "redux-devtools-extension": "^2.13.9"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js"
    ]
  }
}
